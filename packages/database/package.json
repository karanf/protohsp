{
  "name": "@repo/database",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "generate-types": "echo 'InstantDB types are automatically generated. Run: instant-cli init to configure your app.'",
    "dev": "tsc --watch",
    "setup": "tsx src/scripts/setup-instantdb.ts",
    "migrate": "tsx src/scripts/migrate-to-instantdb.ts",
    "migrate-bulk": "tsx src/scripts/run-bulk-migration.ts",
    "migrate-change-queue": "tsx src/scripts/migrate-change-queue.ts",
    "create-placements": "tsx src/scripts/create-placement-data.ts",
    "create-additional-placements": "tsx src/scripts/create-additional-placements.ts",
    "update-avatars": "tsx src/scripts/update-dicebear-avatars.ts",
    "debug-avatars": "tsx src/scripts/debug-specific-avatars.ts",
    "flag-change-queue": "tsx src/scripts/flag-students-for-change-queue.ts",
    "clear-change-queue": "tsx src/scripts/flag-students-for-change-queue.ts clear",
    "populate-current-changes": "tsx src/scripts/populate-current-changes.ts"
  },
  "dependencies": {
    "@instantdb/admin": "^0.20.1",
    "@instantdb/react": "^0.20.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.13.10",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.1",
    "typescript": "^5.8.2"
  }
}